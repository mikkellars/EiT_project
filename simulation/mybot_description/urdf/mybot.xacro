<?xml version='1.0'?>

<robot name="myrobot" xmlns:xacro="http://www.ros.org/wiki/xacro">



  <xacro:include filename="$(find mybot_description)/urdf/mybot.gazebo" />
  <xacro:include filename="$(find mybot_description)/urdf/materials.xacro" />
  <xacro:include filename="$(find mybot_description)/urdf/macros.xacro" />
<!--<link name='base_link'/>
<joint type="fixed" name="world_to_chassis_joint">
<origin xyz="0 0 0" rpy="0 0 0"/>
<parent link="base_link"/>    
<child link="chassis"/>
</joint>-->

  <link name='chassis'>
    <pose>0 0 0.1 0 0 </pose>

    <inertial>
      <mass value="10.0"/>
      <origin xyz="0.0 0 0.06" rpy=" 0 0 0"/>
      <inertia ixx="0.021318375" ixy="0"  ixz="0"
             iyy="0.030650708" iyz="-0.000002"
             izz="0.013549"/>
      </inertial>

    <collision name='collision'>
<origin xyz="0.0 0.0 0.0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".4 .2 .1"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
          <origin xyz="0 0 0.06" rpy="0.0 0.0 -1.57" />
        <geometry>
          <mesh filename="package://mybot_description/meshes/frobit_base.stl" />
        </geometry>
    </visual>


  <!--  <collision name='caster_collision'>
      <origin xyz="-0.15 0 0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.03"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>0.0</slip1>
            <slip2>0.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <visual name='caster_visual'>
      <origin xyz="0.15 0 0.06" rpy=" 0 0 -1.57"/>
      <geometry>
        <mesh filename="package://mybot_description/meshes/front_wheel.stl" />
      </geometry>
    </visual>-->
  </link>
        
<joint name="caster_joint" type="fixed">
            <parent link="chassis"/>
            <child link="caster_link"/>
      </joint>



<link name="caster_link">
   
    <collision name="collision">
      <origin xyz="0.15 0 0.06" rpy=" 0 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.04"/>
      </geometry>
    </collision>
    <visual name="caster_visual">
      <origin xyz="0.15 0 0.06" rpy=" 0 0 -1.57"/>
      <geometry>
        <mesh filename="package://mybot_description/meshes/front_wheel.stl" />
      </geometry>
    </visual>
<inertial>
      <origin xyz="0 0 0.0" rpy="0 1.5707 1.5707"/>
      <mass value="1"/>
      <cylinder_inertia m="1" r="0.05" h="0.04"/>
      <inertia
        ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0"/>
    </inertial>
    
  </link>

  <link name="left_wheel">
    <!--origin xyz="0.1 0.13 0.1" rpy="0 1.5707 1.5707"/-->
    <collision name="collision">
      <origin xyz="0 0 0.0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.05" length="0.04"/>
      </geometry>
    </collision>
    <visual name="left_wheel_visual">
      <origin xyz="0 0 0.0" rpy="1.57 0 1.57"/>
      <geometry>
        <mesh filename="package://mybot_description/meshes/frobit_wheel.stl" />
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0.0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.05" h="0.04"/>
      <inertia
        ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0"/>
    </inertial>
  </link>

  <link name="right_wheel">
    <!--origin xyz="0.1 -0.13 0.1" rpy="0 1.5707 1.5707"/-->
    <collision name="collision">
      <origin xyz="0 0 0.0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.05" length="0.04"/>
      </geometry>
    </collision>
    <visual name="right_wheel_visual">
      <origin xyz="0 0 0.0" rpy="1.57 0 -1.57"/>
      <geometry>
        <mesh filename="package://mybot_description/meshes/frobit_wheel.stl" />
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0.0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.05" h="0.04"/>
      <inertia
        ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0"/>
    </inertial>
  </link>

   <!-- Sensor links -->
  <link name="cam_link">
    <collision name="cam_collision">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </collision>
    <visual name="cam_vis">
      <origin xyz="0 0 0.028" rpy="0 0 -1.5707"/>
      <geometry>
        <mesh filename="package://mybot_description/meshes/sport_camera.dae"/>
      </geometry>
    </visual>
  </link>

  <link name="laser"> 
    <visual> 
      <geometry> 
        <mesh filename="package://mybot_description/meshes/hokuyo.dae" />
      </geometry> 
      <material name="Green"/> 
    </visual> 
  </link>

  <link name="gps_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.01 0.01 0.01" />          
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.01 0.01 0.01" />
      </geometry>
    </collision>
  </link>

  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="-0.07 0.09 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="left_wheel"/>
    
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="1.0" friction="0.1"/>
  </joint>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="-0.07 -0.09 0" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="right_wheel"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="1.0" friction="0.1"/>
  </joint>

  <!-- Sensor joints -->
  <joint type="fixed" name="cam_joint">
    <origin xyz="0 -0.10 0.07" rpy="0 0 -1.5707"/>
    <child link="cam_link"/>
    <parent link="chassis"/>
    <axis xyz="0 0 0" />
 
 <joint name="laser_joint" type="fixed">
    <origin xyz="0.17 0 0.1" rpy="0 0 3.1416" />
    <parent link="chassis" />
    <child link="laser" />
  </joint>

<!--
 <joint name="front_wheel_joint" type="revolute">
    <origin xyz="0.17 0 0.1" rpy="0 0 3.1416" />
    <parent link="chassis" />
    <child link="laser" />
  </joint>
-->


  <joint name="gps_joint" type="fixed">
    <origin xyz="0 0.5 0.07"/>
    <parent link="chassis"/>
    <child link="gps_link"/>
  </joint>



</robot>
